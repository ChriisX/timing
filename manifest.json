{
    "name": "Timing",
    "version": "1.0.0",
    "description": "Displays your navigation times",
    "permissions": ["history", "storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "views/popup.html"
    },
    "icons": {
        "128": "icons/clock.png"
    },
    "manifest_version": 2
}